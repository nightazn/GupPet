				</Anchors>
				<Frames>


					<Frame name="$parentLocations"  inherits="GupPet_OptionsFrameTemplate">
						<Size>
							<AbsDimension x="190" y="410"/>
						</Size>
						<Anchors>
							<Anchor point="TOPLEFT">
								<Offset>
									<AbsDimension x="10" y="-40"/>
								</Offset>
							</Anchor>
						</Anchors>
						<Frames>
							<Button name="$parentTabAdd"    inherits="OptionsFrameTabButtonTemplate" text="Add" id="1">
								<Anchors>
									<Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT">
										<Offset>
											<AbsDimension x="0" y="0"/>
										</Offset>
									</Anchor>
								</Anchors>
								<Scripts>
									<OnLoad>
										self:SetText(GUPPET_L["ADD"][1])
										self.tooltipText = GUPPET_L["ADD"][2]
									</OnLoad>
									<OnClick>
										PlaySound(841); --SOUNDKIT.IG_CHARACTER_INFO_TAB

										-- Get some zone info first --
										local _, instanceType = IsInInstance()
										local ZoneType = 0

										if IsResting() then
											ZoneType = 11

										elseif (   instanceType == "pvp" ) then
											ZoneType = 41

										elseif (  instanceType == "arena" ) then
											ZoneType = 31

										elseif  instanceType == "party" or instanceType == "raid"  then
											ZoneType = 21

										else
											ZoneType = 1

										end


										GupPet_Location( "Add" , GetRealZoneText(), ZoneType )
										GupPet_Interface_UpdateLocationFrame()
									</OnClick>
								</Scripts>
							</Button>
							<Button name="$parentTabRemove"    inherits="OptionsFrameTabButtonTemplate" text="Remove" id="2">
								<Anchors>
									<Anchor point="LEFT" relativeTo="$parentTabAdd" relativePoint="RIGHT">
										<Offset>
											<AbsDimension x="-10" y="0"/>
										</Offset>
									</Anchor>
								</Anchors>
								<Scripts>
