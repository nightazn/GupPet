	<Slider name="GupPet_SliderTemplate" orientation="HORIZONTAL" virtual="true" enableMouse="true">
		<Size>
			<AbsDimension x="50" y="15"/>
		</Size>
		<HitRectInsets>
			<AbsInset left="0" right="-140" top="0" bottom="0"/>
		</HitRectInsets>
<!-- 	
		<Backdrop bgFile="Interface\Buttons\UI-SliderBar-Background" edgeFile="Interface\Buttons\UI-SliderBar-Border" tile="true">
			<EdgeSize>
				<AbsValue val="8"/>
			</EdgeSize>
			<TileSize>
				<AbsValue val="8"/>
			</TileSize>
			<BackgroundInsets>
				<AbsInset left="3" right="3" top="6" bottom="6"/>
			</BackgroundInsets>
		</Backdrop>
-->
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentText" inherits="GupPet_FontText">
					<Anchors>
						<Anchor point="LEFT" relativePoint="RIGHT">
							<Offset x="5" y="0"/>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				self:SetBackdrop({
					bgFile="Interface\Buttons\UI-SliderBar-Background",
					edgeFile="Interface\Buttons\UI-SliderBar-Border",
					tile = true,
					tileEdge = true,
					edgeSize = 8,
					tileSize = 8,
					insets = { left = 3, right = 3, top = 6, bottom = 6 },
				});
				self:ApplyBackdrop();
			</OnLoad>
		</Scripts>
		<ThumbTexture name="$parentThumb" file="Interface\Buttons\UI-SliderBar-Button-Horizontal">
			<Size>
				<AbsDimension x="16" y="32"/>
			</Size>
		</ThumbTexture>
	</Slider>

